<script>
/* âœ… SIDEBAR TOGGLE LOGIC (ONLY ADDITION) */
function toggleSidebar(){
  document.getElementById('sidebar').classList.toggle('hidden');
}

/* ğŸ”¹ AAPKA EXISTING JS BILKUL SAME */
function loadPage(card,url,title){
  document.querySelectorAll('.card').forEach(c=>c.classList.remove('active'));
  card.classList.add('active');

  document.getElementById('titleBar').innerText=title;

  const frame=document.getElementById('webFrame');
  const loader=document.getElementById('loader');

  loader.style.display='block';
  frame.style.opacity='0';
  frame.src=url;

  frame.onload=()=>{
    loader.style.display='none';
    frame.style.opacity='1';
  }
}

function toggleSub(el){
  const sub = el.nextElementSibling;
  document.querySelectorAll('.sub-cards').forEach(s=>{
    if(s !== sub) s.style.display='none';
  });
  sub.style.display = sub.style.display === 'block' ? 'none' : 'block';
}

function filterMenu(){
  const search = document.getElementById('menuSearch').value.toLowerCase();
  const groups = document.querySelectorAll('.group-card');
  const subGroups = document.querySelectorAll('.sub-cards');

  if(search === ''){
    groups.forEach(g=>g.style.display='flex');
    subGroups.forEach(sg=>sg.querySelectorAll('.sub-card')
      .forEach(sc=>sc.style.display='flex')
    );
    return;
  }

  groups.forEach((group,i)=>{
    let found=false;
    const sub=subGroups[i];
    sub.querySelectorAll('.sub-card').forEach(card=>{
      if(card.innerText.toLowerCase().includes(search)){
        card.style.display='flex';
        found=true;
      }else{
        card.style.display='none';
      }
    });
    group.style.display = found ? 'flex' : 'none';
    sub.style.display = found ? 'block' : 'none';
  });
}

window.onload=()=>{
  const home=document.getElementById('homeCard');
  if(home) home.click();
};
</script>

</body>
